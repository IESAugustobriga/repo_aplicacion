<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.infoaugustobriga">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InfoAugustobriga">
        <activity
            android:name=".calendarios.AcCalendarios"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".profesorado.AcProfesorado"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".horarios.AcListaCursos"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".horarios.AcHorarios"
            android:exported="false"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AcMenuPrincipal"
            android:exported="true"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>